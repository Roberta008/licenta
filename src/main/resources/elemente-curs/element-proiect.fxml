<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="containerElement" maxWidth="-Infinity" prefHeight="250.0" prefWidth="1000.0"
      style="-fx-border-color: grey; -fx-border-width: 1; -fx-border-radius: 25; -fx-background-color: white; -fx-background-radius: 25;"
      xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.lucrarelicenta2.controllere.ControllerElementProiect">
    <HBox fx:id="containerTitluSiTermen" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
          VBox.vgrow="ALWAYS"/>
    <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="250.0"
          VBox.vgrow="ALWAYS">
        <VBox fx:id="containerObiectiveProiect" alignment="CENTER" maxHeight="1.7976931348623157E308"
              style="-fx-border-color: #bfbfbf; -fx-border-width: 3; -fx-border-radius: 25; -fx-background-color: transparent;"
              HBox.hgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <HBox.margin>
                <Insets bottom="15.0" left="25.0" right="25.0" top="15.0"/>
            </HBox.margin>
        </VBox>
    </HBox>
    <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0"
          VBox.vgrow="NEVER">
        <Button fx:id="butonIncarcaFisier" mnemonicParsing="false" prefWidth="225.0"
                style="-fx-background-color: #cc33ff; -fx-background-radius: 50; -fx-text-fill: white;"
                text="Incarcare Proiect">
            <font>
                <Font name="System Italic" size="24.0"/>
            </font>
            <VBox.margin>
                <Insets top="15.0"/>
            </VBox.margin>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <HBox.margin>
                <Insets bottom="20.0" top="20.0"/>
            </HBox.margin>
        </Button>
        <Button fx:id="butonPredare" mnemonicParsing="false" prefWidth="225.0"
                style="-fx-background-color: #cc33ff; -fx-background-radius: 50; -fx-text-fill: white;"
                text="Predare Proiect">
            <font>
                <Font name="System Italic" size="24.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="15.0"/>
            </VBox.margin>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <HBox.margin>
                <Insets bottom="20.0" top="20.0"/>
            </HBox.margin>
        </Button>
    </HBox>
    <FlowPane fx:id="containerFisiere" alignment="CENTER" hgap="10.0" vgap="5.0">
        <VBox.margin>
            <Insets bottom="5.0" top="5.0"/>
        </VBox.margin>
    </FlowPane>
    <Label fx:id="labelStatusProiect" alignment="CENTER" maxHeight="1.7976931348623157E308"
           maxWidth="1.7976931348623157E308" style="-fx-text-fill: #cc33ff;" text="Proiect -" VBox.vgrow="NEVER">
        <font>
            <Font name="System Bold Italic" size="36.0"/>
        </font>
        <VBox.margin>
            <Insets bottom="10.0" top="15.0"/>
        </VBox.margin>
    </Label>
</VBox>
